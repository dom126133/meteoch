[forecast_model]

    [forecast_model.deterministic]

        forecast_model.deterministic.url = "https://api.open-meteo.com/v1/forecast"

        [forecast_model.deterministic.model]

            [forecast_model.deterministic.model.best_match]
                models = "best_match"
                name = "Best match"

            [forecast_model.deterministic.model.dwd_icon_eps_seamless]
                models = "icon_seamless"
                name = "DWD Icon Seamless"

            [forecast_model.deterministic.model.ifs_025]
                models = "ecmwf_ifs025"
                name = "ECMWF IFS 0.25°"



    [forecast_model.ensemble]

        url = "https://ensemble-api.open-meteo.com/v1/ensemble"

        [forecast_model.ensemble.model]

            [forecast_model.ensemble.model.dwd_icon_eps_seamless]
                model = "icon_seamless"
                name  = "DWD Icon EPS Seamless"

            [forecast_model.ensemble.model.ifs_025_ens]
                model = "ecmwf_ifs025"
                name = "ECMWF IFS 0.25° Ensemble"
